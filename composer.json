{
    "name": "goodcatch/laravel-module-base",
    "description": "publish base resources",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allen",
            "email": "ali@goodcatch.cn",
            "homepage": "http://note.goodcatch.cn/it/dev-laravel/laravel-modules/laravel-module-base",
            "role": "Developer"
        }
    ],

    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "goodcatch/laravel-modules": "^0.1.0"
    },
    "require-dev": {
        "illuminate/support": "^8.0",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.4.1",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Goodcatch\\Modules\\Base\\": "src"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
